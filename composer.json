
{
    "name": "kubeecms/kui",
    "description": "Admin skin...",
    "keywords": ["kcms-kui", "kui", "skin", "kubeecms", "network", "administration", "menu", "kcms", "kubee"],
    "type": "wordpress-muplugin",
    "config": {
		"sort-packages": true,
		"process-timeout": 0
	},
    "homepage": "https://github.com/KubeeCMS/kui/",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
          "name": "KubeeCMS",
          "homepage": "https://github.com/KubeeCMS/kui/",
          "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/KubeeCMS/kui/issues/",
        "source": "https://github.com/KubeeCMS/kui/"
    },
    "minimum-stability": "dev",    
	"require": {
		"php": ">=8.0",
		"composer/installers": "^1.11",
		"monolog/monolog": "^1.0",
        "psr/cache": "^3.0",
        "psr/container": "^2.0"
	},
	"require-dev": {
		"php": "^7 || ^8",
		"brain/monkey": "^2.0",
		"coenjacobs/mozart": "^0.7",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"mnsami/composer-custom-directory-installer": "^2.0",
		"phpstan/phpstan": "^0.12.98",
		"phpunit/phpunit": "^9.5.9",
		"phpunit/php-file-iterator": "3.0.5",
		"roave/security-advisories": "dev-master",
		"szepeviktor/phpstan-wordpress": "^0.7.7",
        "squizlabs/php_codesniffer": "^3.6.0",
		"wp-coding-standards/wpcs": "^2"
	},
	"autoload": {
		"classmap": [
			"admin/includes",
			"admin/includes/class-admin-2020.php"
		],
		"psr-4": {
			"Admin\\": "admin"
		}
	}
}
